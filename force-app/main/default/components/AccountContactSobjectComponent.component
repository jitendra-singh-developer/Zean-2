<apex:component controller="AccountContactDetailSobjectController" >
    
    <apex:attribute name="objectName" description="" type="String" AssignTo="{!sObjObject}" />
    
    <apex:pageBlockSection columns="1" >
        
        <apex:pageBlockTable value="{!Record}" var="re" >
            <apex:column value="{!re['Name']}" />
            
            <apex:column value="{!re['Phone']}" />
            
        </apex:pageBlockTable>
    </apex:pageBlockSection>
    
</apex:component>