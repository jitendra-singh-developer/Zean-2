<apex:page Controller="AccountListUsingList"  title="Add Account">
    <apex:form >
			<apex:pageBlock >
                <apex:sectionHeader title="Add Account" />
				<apex:pageBlockSection >
                
                    <apex:inputField value="{!acc.Name}" />
                    <apex:inputField value="{!acc.Cities__c}" />
                    <apex:inputField value="{!acc.Rating}" />
                    <apex:inputField value="{!acc.Phone}" />
                    <apex:inputField value="{!acc.Fax}" />
                    <apex:inputField value="{!acc.Website}" />
                     <apex:inputField value="{!acc.Password__c}" />
                
                </apex:pageBlockSection>
                <apex:pageBlockButtons >
                
                    <apex:commandButton action="{!save}" value="Save" />
                
                </apex:pageBlockButtons>
        		
        	</apex:pageBlock>
    
    </apex:form>
</apex:page>