    <apex:form >
        
        <apex:pageBlock >
            <apex:pageBlockSection >
                    <apex:inputField label="Username" value="{!contactLogin.Email}"/> 
                
            
                    <apex:inputField label="Password" value="{!contactLogin.Password__c}" /> 
       
            </apex:pageBlockSection>
                     <apex:pageBlockButtons location="bottom">
                <apex:commandButton value="Login" action="{!doLogin}" style="margin-left:140px" /> <br/><br/>
                <a href="https://ibirdstranniee-developer-edition.ap24.force.com/ibirdscommunity/cusotm_sign_up" style="margin-left:140px"> Signup</a>
                
            </apex:pageBlockButtons>
            
        </apex:pageBlock>
    </apex:form>
</apex:page>