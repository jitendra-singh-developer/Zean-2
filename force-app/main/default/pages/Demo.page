<apex:page title="Demo Page" standardController="Student__c" showHeader="true">
    <apex:form >
    <apex:pageBlock title="Welcome My Friends Welcome...">
 	<apex:pageMessages />
     	
        <apex:pageBlockSection title="Jitendra Singh In">
            
            <apex:inputField value="{!Student__c.First_Name__c}" />
             <apex:inputField value="{!Student__c.Last_Name__c}" />
            <apex:inputField value="{!Student__c.Date_of_Birth__c}" />
            <apex:inputField value="{!Student__c.Email__c}" />
             <apex:inputField value="{!Student__c.HaveWahicle__c}" />
             <apex:inputField value="{!Student__c.Sex__c}" />
             <apex:inputField value="{!Student__c.Married__c}" />
             <apex:inputField value="{!Student__c.WifeName__c}" />
             <apex:inputField value="{!Student__c.Class__c}" />
             <apex:inputField value="{!Student__c.Husband_Name__c}" />
             <apex:inputField value="{!Student__c.Launch__c}" />	
        </apex:pageBlockSection>
        <apex:pageBlockButtons >
        		<apex:commandButton action="{!save}" value="Save" />
            		<apex:commandButton action="{!quicksave}" value="Quick Save" />
            	<apex:commandButton action="{!cancel}"  value="Cancel" />
            <apex:commandButton immediate="true" value="Back" action="/apex/StudentList"/>
            <button>
                <a href="/apex/StudentList" >Back</a>
            </button>
          
           
            
        </apex:pageBlockButtons>
       
		        
    </apex:pageBlock>
    </apex:form>
    
</apex:page>