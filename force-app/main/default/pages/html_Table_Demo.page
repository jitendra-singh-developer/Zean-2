<apex:page controller="AccountPhaseController">
    <apex:form >
    	<apex:pageBlock title="Accounts and Contacts">
            <table border="1">
                <tr>
                	<th>Account Name</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>City</th>
                    <th>State</th>
                </tr>
                <apex:repeat value="{!accountList}" var="a">
                    <tr>
                        <td>{!a.Name}</td>
                        <td>&nbsp;</td>
                        <td>&nbsp;</td>
                        <td>&nbsp;</td>
                        <td>&nbsp;</td>
                    </tr>
                 
                </apex:repeat>
            </table>
        </apex:pageBlock>
    </apex:form>
</apex:page>